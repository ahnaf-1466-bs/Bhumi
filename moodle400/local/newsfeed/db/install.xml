<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="local/newsfeed/db" VERSION="20120122" COMMENT="XMLDB file for newsfeed module"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd">
  <TABLES>
    <TABLE NAME="newsfeed_newsdetails" COMMENT="Each record is one feature data">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="newsimage" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="timecreated" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>


    <TABLE NAME="newsfeed_newsdetailurl" COMMENT="Each record is one feature data">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="newstitle" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="newstitle_bn" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="newssubtitle" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="newssubtitle_bn" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="newsbody" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="newsbody_bn" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="picurl" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="draftid" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="dateofpublish" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="status" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="timecreated" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>