{{!
    @template mod_videoplus/pdfmanage


    Example context (json):
    {

    }
}}


    <div class="localmessage card mt-2 mb-2">
        {{#html}}
            <div class="card-body">
                {{{html}}}
            </div>
        {{/html}}
        {{^html}}
            {{#messages}}
                <div class="card-body">
                    <iframe src="{{pdfurl}}" width="100%" height="500"></iframe><br>

                    <a href="{{featureurl}}&cmid={{cmid}}&messageid={{id}}">
                    {{#pix}} t/edit, core, Edit this section {{/pix}}
                    </a>
                    <a href="{{featureurl}}&cmid={{cmid}}&messageid={{id}}&del=1">
                    {{#pix}} t/delete, core, Edit this section {{/pix}}
                    </a>
                </div>
            {{/messages}}
        {{/html}}
    </div>
{{^messages}}
    <hr>
    <input type="button" class="btn btn-primary" value="{{addpdfbtn}}" onclick="location.href='{{featureurl}}&cmid={{cm_id}}'" />
{{/messages}}
<input type="button" class="btn btn-secondary" value="{{back}}" onclick="location.href='{{backurl}}'">